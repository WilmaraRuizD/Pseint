Algoritmo condicionales_multiples 
		//definir variale 
		
		definir num Como Real;
		
		Escribir "Ingrese un numero : " ;
		leer num ;
		
		si num > 0 Entonces
			Escribir "Numero positivo ";
		SiNo 
			si num == 0 Entonces
				Escribir "El numero es neutro" ;
			SiNo
			Escribir "Numero negativo " ; 
		FinSi
	FinSi
FinAlgoritmo
	

