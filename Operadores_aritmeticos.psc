Algoritmo Operadores_aritmeticos 
	
	Definir a, b, c ,xuno ,xdos como Real ;
	//c = 5 + 3 * 2 / 3 * 3 + 5 ;
	
	Escribir "Ingrese a ";
	Leer a ;
	
	Escribir "Ingrese b " ;
	Leer b ;
	
	Escribir  "Ingrese c " ;
	Leer c ;
	
	xuno =(-b+rc((b^2) - (4 * a * c)))/2*a ;
	xdos =(-b-rc((b^2) - (4 * a * c)))/2*a ;
	
	Escribir "El resultado de xuno es :" xuno ;
	Escribir "El resultado de xdos es : " xdos ;
	
FinAlgoritmo
  