Algoritmo operadores_relacionales
	
	//definir las variables edad
	definir edad Como Entero;
	
	//pedir la edad 
	
	Escribir "Ingrese edad";
	leer edad ;
	Si edad >= 18 Entonces
		Escribir "Puede ingresar ya que es mayor de edad" ;
	SiNo
		Escribir "No tiene permitido el ingreso" ;
	Fin Si
	
	
FinAlgoritmo
